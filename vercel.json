{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/api/vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "backend/api/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "regions": [
    "iad1"
  ],
  "framework": null
}
