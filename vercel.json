{
  "version": 2,
  "buildCommand": "pnpm --filter web build",
  "outputDirectory": "web/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "rootDirectory": "./web",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "video/mp4"
        }
      ]
    }
  ]
}